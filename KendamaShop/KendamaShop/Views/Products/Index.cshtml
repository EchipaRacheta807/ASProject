@{
    ViewBag.Title = "Products Index";
}

<br /> <br /> <br />
<h1>@ViewBag.Message</h1>

<h2>@ViewBag.Title</h2>

<a class="btn btn-primary" href="/Products/New">Add product</a>

@foreach (KendamaShop.Models.Product product in ViewBag.Products)
{
    <div class="panel panel-default">
        @Html.Partial("ProductInfo", product)
        <div class="panel-footer">
            <a class="btn btn-sm btn-succes" href="/Products/Show/@product.ProductId">Show product</a>
        </div>
    </div>
}